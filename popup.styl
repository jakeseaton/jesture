// derp
Yellow = #f9dc8b
Purple = #6a4faf
.title-container
  height 10%

body
  height 500px
  width 500px
  overflow scroll
#popup-banner
  width 100%

h2.issue-title
  font-size 10px
nav
  display inline
  text-align center
  ul
    position relative
    display inline-block !important
  li
    display inline-block !important
    margin-right 0
    margin-left 0
    margin-bottom 0
    max-width 20%
    // width 20%
    border none
    a
      color Purple
      img
        width 36px
        height 36px
      // background-color Yellow
  li.active
    a
      border none
      background-color Purple
      color Yellow

.invisible
  display none
.panel-body
  img
    width 100%
